{
	"name": "anki-importer",
	"description": "Anki deck importer for memorize.ai",
	"scripts": {
		"load-topics": "npm run build && node lib/load-topics --max-old-space-size=24576",
		"load-decks": "npm run build && node lib/load-decks --max-old-space-size=24576",
		"download-decks": "npm run build && node lib/download-decks --max-old-space-size=24576",
		"import-decks": "npm run build && node lib/import-decks --max-old-space-size=24576",
		
		"build": "node_modules/.bin/tsc"
	},
	"dependencies": {
		"firebase-admin": "^8.9.2",
		"lodash": "^4.17.15",
		"mime": "^2.4.4",
		"node-fetch": "^2.6.0",
		"sqlite3": "^4.1.1",
		"system-commands": "^1.1.7",
		"uuid": "^3.4.0"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.149",
		"@types/mime": "^2.0.1",
		"@types/node-fetch": "^2.5.4",
		"@types/sqlite3": "^3.1.6",
		"@types/uuid": "^3.4.6",
		"tslint": "^6.0.0",
		"typescript": "^3.7.5"
	},
	"private": true
}
